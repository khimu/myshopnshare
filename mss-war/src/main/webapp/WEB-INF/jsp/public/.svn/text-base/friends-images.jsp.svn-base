/* =Hoverbox Code
----------------------------------------------------------------------*/
.hoverbox {
	cursor: default;
	list-style: none;
}

.hoverbox a {
	cursor: default;
}

.hoverbox a .preview {
	display: none;
}

.hoverbox a:hover .preview {
	display: block;
	position: relative;
	top: -100px;
	left: -1px;
	z-index: 1;
}

.hoverbox li img {
	background: #fff;
	padding: 5px;
	float: left;
}

.hoverbox li {
	margin: 0px;
	padding: 0px;
	position: relative;
}

.hoverbox li a {
	position: relative;
}

.hoverbox .preview {
	border-color: #000;
	width: 100px;
	height: 150px;
}
</style>


