<%@ tag isELIgnored="false" %><%@ attribute name="idType" required="true" rtexprvalue="true" %><%@ attribute name="deleteURL" required="true" rtexprvalue="true" %><%@ attribute name="isPrimary" required="false" rtexprvalue="true" %><%@ attribute name="primaryKey" required="true" rtexprvalue="true" %><%@ attribute name="records" required="true" rtexprvalue="true" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><tr><c:forTokens items="<%=records%>" delims="|" var="record"><td>${record}</td></c:forTokens><td><c:if test="${not empty isPrimary and isPrimary == true}">Primary</c:if></td><td id="delete_${idType}_${primaryKey}"><a href="javascript: void(0);" onclick="delete_record(${primaryKey}, \'${idType}\');">Delete</a><div style="display: none;">${deleteURL}</div></td><td id="edit_${idType}_${primaryKey}"><a href="javascript: void(0);" onclick="edit_record(${primaryKey}, \'${idType}\');">Edit</a><div style="display: none;">${deleteURL}</div></td></tr>