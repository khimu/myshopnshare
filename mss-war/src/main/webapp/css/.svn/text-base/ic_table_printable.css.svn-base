.tabularTable table
{
	background: #C0C0C0;
	border-collapse: collapse;
	clear: left;
	width: 100%;
}
.tabularTable table td
{
	padding: 2px;
	border: 1px solid #C0C0C0;
}
.tabularTable table th
{
	padding: 2px;
	border: 1px solid #C0C0C0;
}
.tabularTable table thead tr th
{
	font-family: Arial, Helvetica, SansSerif;
	color: #003399;
	font-size: 8pt;
	background: #FFFFFF;
}
.tabularTable table thead tr th a
{
	color: #003399;
}
.tabularTable .odd td
{
	font-family: Arial, Helvetica, SansSerif;
	color: #000000;
	font-size: 8pt;
	background: #CCCCFF;
}
.tabularTable .even td
{
	font-family: Arial, Helvetica, SansSerif;
	color: #000000;
	font-size: 8pt;
	background: #FFFFFF;
}
.tabularTable table tfoot td
{
	font-family: Arial, Helvetica, SansSerif;
	color: #000000;
	font-size: 8pt;
	font-weight: bold;
	background: #CCCCCC;
}
.tabularTable .pagebanner
{
	font-family: Arial, Helvetica, SansSerif;
	color: #000000;
	font-size: 8pt;
	font-weight: normal;
	float: left;
	display: block;
}
.tabularTable .pagelinks
{
	color: #333333; 
	font: 11px Arial, Helvetica, sans-serif; 
	font-weight: normal;
	float: right;
	display: block;
}
.tabularTable .pagelinks a
{
	color: #666666;
}
.tabularTable .pagelinks .disabledLinks
{
	color: #666666;
}
.tabularTable .exportlinks
{
	display: none;
}
.tabularTable .sorted a
{
	color: #cc0000;
}

hr
{
	clear: left;
}

