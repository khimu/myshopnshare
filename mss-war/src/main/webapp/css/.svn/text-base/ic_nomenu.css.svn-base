/**
  * POSTAL-STYLE STYLE SHEETS (AS APPROVED BY THE INTERNET CHANNEL)
  * For No Left Menu Bar
  */
.topMenuBar
{
	width:100%;
	position: relative;
	top: 1px;
	height: 25px;
	padding: 0;
	margin: 0;
	font-size: 0pt;
	background-image: url('/govgrnds-example/images/top_bar3.gif');
}
.navBar
{
	width: 691px;
	background-image: url('/govgrnds-example/images/top_nav1_long.gif');
	height: 18px;
	display: block;
	position: relative;
	left: 29px;
	padding: 0;
	top: 3px;
}
.navBar a
{
	background-image: url('/govgrnds-example/images/top_nav2.gif');
	display: block;
	float: right;
	FONT: bold 11px Arial, Helvetica, sans-serif; 
	COLOR: #003399;
	height: 18px;
	width: 263px;
	text-align: center;
}
h1
{
	margin-top: 10px;
	margin-bottom: 0;
	margin-left: 44px;
	padding: 0;	
}
.h1_text
{
	color: #000099;
	font-family: "Arial Narrow", Arial, Helvetica, SansSerif;
	font-size: 24px;
	margin: 0;
	padding: 0;
}
.bodyFrame
{
	margin-left: 44px;
	width: 675px;
